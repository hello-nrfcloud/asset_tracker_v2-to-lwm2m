{
  "name": "@hello.nrfcloud.com/asset_tracker_v2-to-lwm2m",
  "version": "0.0.0-development",
  "description": "Converts asset_tracker_v2 messages to LwM2M objects",
  "type": "module",
  "scripts": {
    "test": "npx globstar -- npx tsx --test --test-reporter spec \"!(dist|node_modules)/**/*.spec.ts\"",
    "prepare": "husky && check-node-version --package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hello-nrfcloud/asset_tracker_v2-to-lwm2m.git"
  },
  "bugs": {
    "url": "https://github.com/hello-nrfcloud/asset_tracker_v2-to-lwm2m/issues"
  },
  "homepage": "https://github.com/hello-nrfcloud/asset_tracker_v2-to-lwm2m#readme",
  "keywords": [
    "nordicsemiconductor",
    "cellular-iot",
    "hello-nrfcloud",
    "aws",
    "cdk"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@aws-sdk/client-iam": "3.650.0",
    "@aws-sdk/client-iot-data-plane": "3.650.0",
    "@bifravst/aws-cdk-lambda-helpers": "1.9.52",
    "@bifravst/ci": "2.0.2",
    "@bifravst/eslint-config-typescript": "6.1.15",
    "@bifravst/prettier-config": "1.1.0",
    "@commitlint/config-conventional": "19.4.1",
    "@types/node": "22.5.4",
    "aws-cdk-lib": "2.158.0",
    "cdk": "2.158.0",
    "check-node-version": "4.2.1",
    "globstar": "1.0.0",
    "husky": "9.1.6",
    "prettier": "3.3.3",
    "tsx": "4.19.1",
    "typescript": "5.6.2"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "prettier": "@bifravst/prettier-config",
  "dependencies": {
    "@aws-lambda-powertools/metrics": "2.7.0",
    "@hello.nrfcloud.com/lambda-helpers": "2.0.23",
    "@hello.nrfcloud.com/proto-map": "16.1.5",
    "@middy/core": "5.4.7"
  }
}
